{
  "name": "gthanks",
  "packageManager": "pnpm@10.20.0",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node scripts/ensure-env.js && node scripts/ensure-db.js && next dev",
    "dev:inspect": "node scripts/ensure-env.js && node scripts/ensure-db.js && NODE_OPTIONS='--inspect' next dev",
    "dev:inspect-brk": "node scripts/ensure-env.js && node scripts/ensure-db.js && NODE_OPTIONS='--inspect-brk' next dev",
    "dev:memory": "node scripts/ensure-env.js && node scripts/ensure-db.js && NODE_OPTIONS='--inspect --max-old-space-size=4096' next dev",
    "build": "node scripts/ensure-env.js && node scripts/ensure-db.js && next build",
    "setup": "node scripts/setup-env.js",
    "generate-secret": "node scripts/generate-secret.js",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "next lint --max-warnings 300",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings=0",
    "lint:service-layer": "eslint 'src/app/api/**/*.ts' --rule 'local-rules/no-direct-db-import: error' --rule 'local-rules/use-permission-service: error'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "db:push": "node -e \"if(process.cwd().endsWith('/prisma')){console.error('ERROR: Run from project root, not prisma/ subdirectory');process.exit(1)}\" && prisma db push",
    "db:studio": "node -e \"if(process.cwd().endsWith('/prisma')){console.error('ERROR: Run from project root, not prisma/ subdirectory');process.exit(1)}\" && prisma studio",
    "db:generate": "node -e \"if(process.cwd().endsWith('/prisma')){console.error('ERROR: Run from project root, not prisma/ subdirectory');process.exit(1)}\" && prisma generate",
    "seed": "DATABASE_URL=\"file:$(pwd)/data/gthanks.db\" prisma db seed",
    "seed:clean": "DATABASE_URL=\"file:$(pwd)/data/gthanks.db\" prisma db seed -- --clean",
    "db:reset:seed": "rm -rf data/*.db && node scripts/ensure-db.js && DATABASE_URL=\"file:$(pwd)/data/gthanks.db\" prisma db seed",
    "dev:clean": "rm -rf data/*.db && node scripts/ensure-db.js && next dev",
    "db:ensure": "node scripts/ensure-db.js",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:status": "prisma migrate status",
    "migrate:diff": "prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma",
    "migrate:reset": "prisma migrate reset",
    "migrate:check": "bash scripts/check-migrations.sh",
    "postinstall": "prisma generate && husky",
    "prepare": "husky",
    "test:integration": "jest --config jest.config.integration.js",
    "test:integration:watch": "jest --config jest.config.integration.js --watch",
    "test:all": "npm run test && npm run test:integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@node-rs/argon2": "^2.0.2",
    "@prisma/client": "^5.15.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.80.6",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/jsdom": "^27.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "exifr": "^7.1.3",
    "file-type": "^19.0.0",
    "jsdom": "^27.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.290.0",
    "next": "14.2.3",
    "next-auth": "4.24.10",
    "next-themes": "^0.4.6",
    "node-cache": "^5.1.2",
    "nodemailer": "6.9.16",
    "rate-limiter-flexible": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^5.5.3",
    "react-hook-form": "^7.58.0",
    "resend": "^6.0.1",
    "sanitize-html": "^2.17.0",
    "sharp": "^0.34.2",
    "simple-icons": "^15.11.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/sanitize-html": "^2.16.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "autoprefixer": "^10.4.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "lint-staged": "^16.1.5",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
